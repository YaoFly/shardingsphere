<e2e-test-cases>

    <test-case sql="SELECT GROUP_CONCAT(o.order_id) as order_id_group_concat FROM t_order o where o.order_id in (?, ?, ?)" db-types="MySQL" scenario-types="db,tbl,dbtbl_with_readwrite_splitting,readwrite_splitting,db_tbl_sql_federation">
        <assertion parameters="1001:int, 1100:int, 1600:int" expected-data-file="select_group_concat.xml" />
    </test-case>

    <test-case sql="SELECT GROUP_CONCAT(distinct o.merchant_id SEPARATOR ' ') as order_id_group_concat FROM t_order o where o.order_id in (?, ?, ?)" db-types="MySQL" scenario-types="db,tbl,dbtbl_with_readwrite_splitting,readwrite_splitting,db_tbl_sql_federation">
        <assertion parameters="1001:int, 1100:int, 1600:int" expected-data-file="select_group_concat_distinct.xml" />
    </test-case>
</e2e-test-cases>
